{
  "AllowedHosts": "*",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Error"
    }
  },
  "general": {
    "name": "FollowUP",
    "seedData": true
  },
  "jwt": {
    "issuer": "https://localhost:5000",
    "audience": "https://localhost:5000",
    "expiryMinutes": 5
  },
  "sql": {
    "inMemory": false
  },
  "promotion": {
    "enabled": true,
    "updateComments": false,
    "useEmbeddedBrowser": false,
    "headlessBrowser": true,
    "minActionInterval": 60000,
    "maxActionInterval": 90000,
    "minIntervalDifference": 5000,
    "banDurationInDays": 7,
    "minDaysToUnfollow": 5,
    "promotionKey": "-latest-promotion",
    "activityKey": "-activity",
    "searchKey": "-media"
  }
}
