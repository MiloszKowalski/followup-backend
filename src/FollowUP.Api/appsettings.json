{
  "AllowedHosts": "*",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Error"
    }
  },
  "general": {
    "name": "FollowUP",
    "seedData": true
  },
  "jwt": {
    "issuer": "https://localhost:5000",
    "audience": "https://localhost:5000",
    "expiryMinutes": 5
  },
  "sql": {
    "inMemory": false
  },
  "promotion": {
    "enabled": true,
    "updateComments": false,
    "startingHour": 9,
    "endingHour": 23,
    "headlessBrowser": true,
    "minActionInterval": 60000,
    "maxActionInterval": 80000,
    "minIntervalDifference": 5000,
    "minTooManyActionsInterval": 600000,
    "maxTooManyActionsInterval": 1200000,
    "banDurationInDays": 7,
    "minDaysToUnfollow": 5,
    "followChance": 90,
    "unfollowChance": 40,
    "unfollowAfterPositiveActionsLimits": true,
    "exploreFeedChance": 80,
    "activityFeedChance": 80,
    "promotionKey": "-latest-promotion",
    "activityKey": "-activity",
    "searchKey": "-media"
  },
  "email": {
    "sendFromEmail": "biuro@followup.social",
    "sendFromName": "FollowUP"
  },
  "api": {
    "dashboardBaseUrl": "app.followup.social",
    "apiBaseUrl": "followup.social/api/"
  },
  "instaLogger": {
    "logLevel": 0,
    "logToFile": true
  }
}
